        "type": "string"
      },
      "creator_id": {
        "type": "string"
      },
      "gross_amount": {
        "type": "number"
      },
      "fee_amount": {
        "type": "number"
      },
      "net_amount": {
        "type": "number"
      },
      "currency": {
        "type": "string"
      },
      "paid_at": {
        "type": "string",
